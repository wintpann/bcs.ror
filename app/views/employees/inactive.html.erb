<% if @inactive_employees %>
<h2 class="page-title">Удаленные работники</h2>
  <div class="collection">
    <ul>
      <% @inactive_employees.each do |employee| %>
        <li><%= link_to employee.name, user_employee_path(params[:user_id], employee.id) %></li>
      <% end %>
    </ul>
  </div>
<% end %>
