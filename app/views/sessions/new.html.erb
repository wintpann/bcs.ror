<h2>Log in</h2>
<%= form_for :session, url: login_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :remember_me %>
    <%= f.check_box :remember_me %>
  </div>
  <%= f.submit 'Log in' %>
<% end %>
